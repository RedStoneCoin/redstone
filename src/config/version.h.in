#define STR_HELPER(x) #x
#define STR(x) STR_HELPER(x)

#define PROJECT_NAME "Redstone project"
#define PROJECT_SITE "https://github.com/RedStoneCoin/redstone"
#define PROJECT_COPYRIGHT "Copyright 2020, The RedStone Developers"
#define APP_VER_MAJOR 3 // Current global network version
#define APP_VER_MINOR 1 // This will change when you do a hard/soft fork
#define APP_VER_REV 1 // This is the version you change every time you do some small changes,
#define APP_VER_BUILD 4 // Everytime you build something even for testing or whatever, bump this by 1
#define BUILD_COMMIT_ID "3.1"//Change it
#define PROJECT_VERSION STR(APP_VER_MAJOR) "." STR(APP_VER_MINOR) "." STR(APP_VER_REV)
#define PROJECT_VERSION_BUILD_NO STR(APP_VER_BUILD)
#define PROJECT_VERSION_LONG PROJECT_VERSION "." PROJECT_VERSION_BUILD_NO
#define PROJECT_DESCRIPTION PROJECT_NAME " " PROJECT_VERSION_LONG
